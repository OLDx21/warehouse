<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.newpr">

    <application
            android:allowBackup="true"
            android:icon="@drawable/iconwho"
            android:label="@string/app_name"
            android:roundIcon="@drawable/iconwho"
            android:supportsRtl="true"
            android:theme="@style/Theme.NewPr">
        <activity android:name=".AddTovar"></activity>
        <activity android:name=".UpdateProduct"></activity>
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:theme="@style/Theme.NewPr.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>